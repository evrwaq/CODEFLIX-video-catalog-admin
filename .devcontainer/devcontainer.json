{
  "name": "CODEFLIX - Video Catalog Admin",
  "dockerComposeFile": [
    "../docker-compose.yaml",
    "docker-compose.yml"
  ],
  "service": "app",
  "workspaceFolder": "/home/node/app",
  "features": {
    "ghcr.io/devcontainers/features/common-utils": {},
    "ghcr.io/devcontainers-contrib/features/zsh-plugins": {
      "plugins": "git git-flow F-Sy-H zsh-autosuggestions zsh-completions",
      "omzPlugins": "https://github.com/z-shell/F-Sy-H https://github.com/zsh-users/zsh-autosuggestions https://github.com/zsh-users/zsh-completions"
    },
    "ghcr.io/stuartleeks/dev-container-features/shell-history": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ]
    }
  }
}
